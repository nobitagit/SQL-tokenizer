{
  "name": "sql-tokenizer",
  "version": "0.2.1",
  "description": "analyzes a SQL statement and converts it into a list of tokens",
  "main": "index.js",
  "scripts": {
    "tsc--noemit": "tsc --declaration --project . --noemit",
    "tslint": "tslint -c tslint.json '{src,test}/**/*.{ts,tsx}'",
    "postversion": "npm publish; git push origin v${npm_package_version}; git push origin master",
    "test": "npm run tape; npm run tsc--noemit",
    "tape": "ts-node node_modules/.bin/tape test/*.ts"
  },
  "pre-commit": [
    "test",
    "tslint"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fibo/SQL-tokenizer.git"
  },
  "keywords": [
    "SQL",
    "tokenizer"
  ],
  "author": "Gianluca Casati (http://g14n.info)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fibo/SQL-tokenizer/issues"
  },
  "homepage": "http://g14n.info/SQL-tokenizer",
  "devDependencies": {
    "@types/tape": "^4.2.33",
    "dot-editorconfig": "^1.1.1",
    "pre-commit": "^1.2.2",
    "tape": "^4.10.1",
    "ts-node": "^8.0.2",
    "tslint": "^5.12.1",
    "typescript": "^3.3.3"
  },
  "dependencies": {
    "sql92-operators": "^1.0.2"
  }
}
